Процедура ОбработкаПроведения(Отказ, РежимПроведения) 
	// регистр гисЗначенияПоказателейОбъектов
	Движения.гисЗначенияПоказателейОбъектов.Записывать = Истина;
	Для Каждого СтрокаПоказателей Из Значения Цикл
		Движение = Движения.гисЗначенияПоказателейОбъектов.Добавить();
		Движение.Период = Дата;
		Движение.Объект = ОбъектКарты;
		Движение.Показатель = СтрокаПоказателей.Показатель;
		
		Если СтрокаПоказателей.Показатель.ТипЗначения = Перечисления.гисПоказателиТипы.Число Тогда
			Движение.ЗначениеЧисло = СтрокаПоказателей.Значение;
		ИначеЕсли СтрокаПоказателей.Показатель.ТипЗначения = Перечисления.гисПоказателиТипы.Булево Тогда
			Движение.ЗначениеБулево = СтрокаПоказателей.Значение;
		ИначеЕсли СтрокаПоказателей.Показатель.ТипЗначения = Перечисления.гисПоказателиТипы.Справочник Тогда
			Движение.ЗначениеСправочник = СтрокаПоказателей.Значение;
		КонецЕсли;
	КонецЦикла;
КонецПроцедуры

