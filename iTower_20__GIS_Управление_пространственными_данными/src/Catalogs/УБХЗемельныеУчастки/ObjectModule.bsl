	ВидыГосРегистрацииПрава 		= "";
	НомераГосРегистрацииПрава	 	= "";
	ДатыГосРегистрацииПрава 		= "";
	
	ВидыОбременений 				= "";
	ДатыГосРегистрацииОбременений 	= "";
	НомераГосРегистрацииОбременений = "";
	СрокиОбременений 				= "";
