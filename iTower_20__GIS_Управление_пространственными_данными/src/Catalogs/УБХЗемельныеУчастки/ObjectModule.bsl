	Для Каждого СтрокаТЧ Из ПраваЗУ Цикл
		Правообладатели 			= Правообладатели 			+ ?(Правообладатели = "", "", "; ") 			+ СтрокаТЧ.Правообладатель;
		ВидыГосРегистрацииПрава 	= ВидыГосРегистрацииПрава 	+ ?(ВидыГосРегистрацииПрава = "", "", "; ")		+ СтрокаТЧ.ВидГосРегистрацииПрава;
