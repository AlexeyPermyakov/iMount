		ДатыГосРегистрацииПрава 	= ДатыГосРегистрацииПрава 	+ ?(ДатыГосРегистрацииПрава = "", "", "; ") 	+ СтрокаТЧ.ДатаГосРегистрацииПрава;
	КонецЦикла;
	
	Для Каждого СтрокаТЧ Из ОбремененияЗУ Цикл
		ВидыОбременений 				= ВидыОбременений 					+ ?(ВидыОбременений = "", "", "; ") 				+ СтрокаТЧ.ВидОбременения;
		ДатыГосРегистрацииОбременений 	= ДатыГосРегистрацииОбременений 	+ ?(ДатыГосРегистрацииОбременений = "", "", "; ") 	+ СтрокаТЧ.ДатаГосРегистрацииОбременения;
		НомераГосРегистрацииОбременений = НомераГосРегистрацииОбременений 	+ ?(НомераГосРегистрацииОбременений = "", "", "; ") + СтрокаТЧ.НомерГосРегистрацииОбременения;
		СрокиОбременений 				= СрокиОбременений 					+ ?(СрокиОбременений = "", "", "; ") 				+ СтрокаТЧ.СрокОбременения;
		ЛицаОбременений 				= ЛицаОбременений 					+ ?(ЛицаОбременений = "", "", "; ") 				+ СтрокаТЧ.ЛицоОбременения;
		ОснованияРегистрацииОбременений = ОснованияРегистрацииОбременений 	+ ?(ОснованияРегистрацииОбременений = "", "", "; ") + СтрокаТЧ.ОснованиеРегистрацииОбременения;
	КонецЦикла;
	
